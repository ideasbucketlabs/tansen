{
    "name": "tansen",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/",
        "test:unit:ci": "vitest run --environment jsdom --root src/",
        "test:unit:coverage": "vitest --coverage --environment jsdom --root src/",
        "test:unit:coverage:ci": "vitest run --coverage --environment jsdom --root src/",
        "test:e2e": "playwright test",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
    },
    "dependencies": {
        "@codemirror/lang-json": "^6.0.0",
        "@codemirror/legacy-modes": "^6.1.0",
        "@codemirror/lint": "^6.0.0",
        "@popperjs/core": "^2.11.5",
        "codemirror": "^6.0.0",
        "diff-match-patch": "^1.0.5",
        "file-saver": "^2.0.5",
        "lodash.debounce": "^4.0.8",
        "lodash.drop": "^4.1.1",
        "lodash.filter": "^4.6.0",
        "lodash.isequal": "^4.5.0",
        "lodash.orderby": "^4.6.0",
        "pinia": "^2.0.21",
        "reconnecting-eventsource": "^1.5.0",
        "vue": "^3.2.38",
        "vue-router": "^4.1.5"
    },
    "devDependencies": {
        "@playwright/test": "^1.28.1",
        "@rushstack/eslint-patch": "^1.1.4",
        "@tailwindcss/forms": "^0.5.1",
        "@types/diff-match-patch": "^1.0.32",
        "@types/file-saver": "^2.0.5",
        "@types/jsdom": "^20.0.1",
        "@types/lodash": "^4.14.184",
        "@types/lodash.debounce": "^4.0.7",
        "@types/lodash.drop": "^4.1.7",
        "@types/lodash.filter": "^4.6.7",
        "@types/lodash.isequal": "^4.5.6",
        "@types/lodash.orderby": "^4.6.7",
        "@types/node": "^18.11.12",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vitest/coverage-c8": "^0.24.3",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^11.0.0",
        "@vue/test-utils": "^2.2.6",
        "@vue/tsconfig": "^0.1.3",
        "autoprefixer": "^10.4.12",
        "eslint": "^8.22.0",
        "eslint-plugin-vue": "^9.3.0",
        "jsdom": "^20.0.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.18",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.2.1",
        "tailwindcss": "^3.1.8",
        "typescript": "~4.7.4",
        "vite": "^4.0.0",
        "vitest": "^0.25.6",
        "vue-tsc": "^1.0.12"
    }
}
